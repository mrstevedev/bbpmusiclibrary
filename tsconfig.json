{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["./src/components/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/query/*": ["./src/query/*"],
      "@/product/*": ["./product/*"],
      "@/styles/*": ["./styles/*"],
      "@/util/*": ["./src/util/*"],
      "@/services/*": ["./src/services/*"],
      "@/public/*": ["./public/*"],
      // "@/components/Context/*": ["context/*"]
      "@/context/*": ["./src/context/*"],
      "@/types/*": ["./types/*"]
    },
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "../bbpmusic_library_backup/pages/_app.tsx",
    "../bbpmusic_library_backup/pages/index.tsx",
    "../bbpmusic_library_backup/components/Navigation/OverlayNav.tsx",
    "../bbpmusic_library_backup/components/Sidebar/SidebarCart.tsx",
    "../bbpmusic_library_backup/pages_router/confirm.tsx",
    "../bbpmusic_library_backup/components/Sidebar/Cart.tsx",
    "../bbpmusic_library_backup/components/Sidebar/CartItem.tsx",
    "../bbpmusic_library_backup/components/Hero/ImageHero.tsx",
    "util/generate.ts",
    "emails/Purchased.jsx",
    ".next/types/**/*.ts",
    "../bbpmusic_library_backup/pages_router/login.tsx",
    "../bbpmusic_library_backup/pages_router/profile/orders.tsx",
    "../bbpmusic_library_backup/pages_router/profile/downloads.tsx",
    "../bbpmusic_library_backup/about.tsx",
    "src/util/index.js",
    "../bbpmusic_library_backup/pages_router/create-password.tsx",
    "../bbpmusic_library_backup/pages_router/profile/update.tsx",
    "../bbpmusic_library_backup/pages_router/forgot-password.tsx",
    "../bbpmusic_library_backup/pages_router/checkout.tsx",
    "../bbpmusic_library_backup/pages_router/checkout.tsx",
    "../bbpmusic_library_backup/pages_router/register.tsx"
  ],
  "exclude": ["node_modules"]
}
